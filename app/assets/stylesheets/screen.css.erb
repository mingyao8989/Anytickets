/*------------------------------------------
// 4.4  CUSTOM SEARCH  PAGE SPECIFIC STYLES
//------------------------------------------*/

/*---- Filter Results ---*/
.filter-results h2 { margin-bottom:9px;}
.filter-results h2 span { background:url(<%= asset_path "icons.png" %>) left -370px no-repeat; line-height:22px;}
.filter-results ul { margin-bottom:0px;}
.filter-results ul li { margin-bottom:2px;}
.filter-results ul li.active a { color:#888888;}
.filter-results .selected-filters { border-bottom:1px solid #dfdfdf; border-top:1px solid #dfdfdf; padding:7px 0px 6px; margin-bottom:18px;}
.filter-results .selected-filters h4 { font-weight:bold; font-size:12px; color:#989898; margin:0px 0px 5px;}
.filter-results .selected-filters ul li a.remove { color:#d50000; text-decoration:none;}

.filter-results .section { margin-bottom:16px;}
.filter-results a.more { font-size:12px; color:#829904; line-height:normal;}

.filter-results .by-dates ul { margin-bottom:10px;}
.filter-results .by-dates p { margin-bottom:0px;}
.filter-results .by-dates p input.textbox { float:left; padding:0px 5px; border:1px solid #dfdfdf; height:25px; line-height:25px; width:64px; -webkit-box-shadow:none; -moz-box-shadow:none; box-shadow:none; -webkit-border-radius:0px; -moz-border-radius:0px; border-radius:0px; margin:0px;}
.filter-results .by-dates p span.to { display:block; float:left; font-size:12px; color:#333333; margin:0px 9px; line-height:25px;}

/*---- Search Results ---*/
.search-results .sort-by label { font-size:13px; margin-right:10px; float:left; line-height:25px;}
.search-results .sort-by select { border:1px solid #dfdfdf; width:182px; font-size:13px; height:27px; line-height:27px; color:#9a9a9a; -webkit-border-radius:0px; -moz-border-radius:0px; border-radius:0px; -webkit-box-shadow:none; -moz-box-shadow:none; box-shadow:none; padding:3px 4px;}
.search-results .table1 tr th { border-width:0px 0 1px; padding:0px 10px 3px;}
.search-results .table1 tr td { padding:8px 10px; line-height:16px;}
.search-results .table1 tr td a { color:#829904; font-size:13px; font-weight:normal;}
.table1 tr td a.button1{color:#fff;}

.search-results .table1 tr td.event, .search-results .table1 tr td.venue { width:125px;}
.search-results .table1 tr td.date { width:90px;}
.search-results .table1 tr td.min-price { width:100px;}
.search-results .table1 tr td.city { width:88px;}

.search-results .pagination { margin-top:5px; }

/*---- Box4 ---*/
.box4 { position:relative; border:1px solid #d3d3d3; background:#fff; -webkit-border-radius:5px; -moz-border-radius:5px; border-radius:5px;}


/*---- Pagination ---*/
.pagination{margin-bottom:10px;}
.box3 .pagination{margin-bottom:0;}
.pagination ul { position:relative; -webkit-box-shadow: 0 1px 2px #ededed;  -moz-box-shadow: 0 1px 2px #ededed;     box-shadow: 0 1px 2px #ededed; }
.pagination ul > li > a, .pagination ul > li > span { border:0px; font-size:13px;}
.pagination ul > li > a { color:#4a6db6;}
.pagination a:hover, .pagination .active a, .pagination .active span { color:#999999;}
.pagination a.previous:hover, .pagination a.next:hover { color:#333333; background:none;}




